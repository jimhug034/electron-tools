  "lint-staged": {
    "src/**/*.ts":["eslint --fix", "git add"]
  },
  "author": "jimhug",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@types/electron": "^1.6.10",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "electron": "^13.2.1"
  }
}
