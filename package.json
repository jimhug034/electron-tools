{
  "name": "electron-tools",
  "version": "1.0.0",
  "private":false,
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "precommit": "lint-staged",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "start": "npm run build && electron ./dist/main.js"
